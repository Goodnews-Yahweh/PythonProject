#!/bin/python

import random
from getpass import getpass
import string

usr_pwd = getpass("Enter password: ")
print(usr_pwd)
passwd = string.punctuation + string.digits + string.ascii_letters
crack=""

while(crack != usr_pwd and not (usr_pwd.isspace())):
    crack=""
    for i in range(len(usr_pwd)):
        guess_pwd = passwd[random.randint(0, len(passwd) - 1)]
        # pwd[randint(0,17)]
        pwd = str(guess_pwd)
        crack += pwd
        print(crack)
        print("Cracking Password...Please wait")


print(f"Your Password is: {crack}")
